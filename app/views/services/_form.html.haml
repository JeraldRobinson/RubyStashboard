= form_for(@service) do |f|
  = render 'shared/error_messages', :object => f.object
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .field
    = f.label :description
    %br/
    = f.text_field :description
  .field
    = f.label :url, "url"
    %br/
    = f.text_field :url

  .actions
    = f.submit "Create"